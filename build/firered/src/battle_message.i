u32 BattleStringExpandPlaceholders(const u8 *src, u8 *dst)
{
    u32 dstId = 0;
    const u8 *toCpy = ((void *)0);
    u8 text[30];
    u8 multiplayerId;
    s32 i;

    multiplayerId = GetMultiplayerId();

    while (*src != 0xFF)
    {
        if (*src == 0xFD)
        {
            src++;
