      || mapNum == ((89 | (1 << 8)) & 0xFF)
      || mapNum == ((90 | (1 << 8)) & 0xFF)
      || mapNum == ((91 | (1 << 8)) & 0xFF)
      || mapNum == ((92 | (1 << 8)) & 0xFF)
      || mapNum == ((93 | (1 << 8)) & 0xFF)
