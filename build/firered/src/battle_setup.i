void PlayTrainerEncounterMusic(void)
{
    u16 music;

    if (!(gQuestLogState == 2 || gQuestLogState == 3)
     && sTrainerBattleMode != 1
     && sTrainerBattleMode != 8)
    {
        switch (GetTrainerEncounterMusicId(gTrainerBattleOpponent_A))
        {
        case 1:
        case 2:
        case 9:
            music = 284;
            break;
        case 0:
        case 4:
        case 5:
        case 8:
        case 10:
        case 11:
        case 12:
        case 13:
            music = 285;
            break;
        default:
            music = 283;
            break;
        }
        PlayNewMapMusic(music);
    }
}

