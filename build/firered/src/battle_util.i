                        { gBattleTextBuff1[0] = 0xFD; gBattleTextBuff1[1] = 2; gBattleTextBuff1[2] = 113; gBattleTextBuff1[3] = (113 & 0xFF00) >> 8; gBattleTextBuff1[4] = 0xFF; };
