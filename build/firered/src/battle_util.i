                    if (gBattleMoveDamage == 0)
                        gBattleMoveDamage = 1;
                    if ((gBattleMons[gActiveBattler].status1 & (1 << 8 | 1 << 9 | 1 << 10 | 1 << 11)) != ((15) << 8))
                        gBattleMons[gActiveBattler].status1 += ((1) << 8);
                    gBattleMoveDamage *= (gBattleMons[gActiveBattler].status1 & (1 << 8 | 1 << 9 | 1 << 10 | 1 << 11)) >> 8;
