                        BattleScriptExecute(BattleScript_SideStatusWoreOff);
                        { gBattleTextBuff1[0] = 0xFD; gBattleTextBuff1[1] = 2; gBattleTextBuff1[2] = 115; gBattleTextBuff1[3] = (115 & 0xFF00) >> 8; gBattleTextBuff1[4] = 0xFF; };
