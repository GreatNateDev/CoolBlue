bool8 ShouldIgnoreDeoxysForm(u8 caseId, u8 battlerId)
{
    switch (caseId)
    {
    case 0:
    default:
        return 0;
    case 1:
        if (!(gBattleTypeFlags & (1 << 6)))
            return 0;
        if (!gMain.inBattle)
            return 0;
        if (gLinkPlayers[GetMultiplayerId()].id == battlerId)
            return 0;
        break;
    case 2:
        break;
    case 3:
        if (!(gBattleTypeFlags & (1 << 6)))
            return 0;
        if (!gMain.inBattle)
            return 0;
        if (battlerId == 1 || battlerId == 4 || battlerId == 5)
            return 1;
        return 0;
    case 4:
        break;
    case 5:
        if (gBattleTypeFlags & (1 << 1))
        {
            if (!gMain.inBattle)
                return 0;
            if (gBattleTypeFlags & (1 << 6))
            {
                if (gLinkPlayers[GetMultiplayerId()].id == battlerId)
                    return 0;
            }
            else
            {
                if (GetBattlerSide(battlerId) == 0)
                    return 0;
            }
        }
        else
        {
            if (!gMain.inBattle)
                return 0;
            if (GetBattlerSide(battlerId) == 0)
                return 0;
        }
        break;
    }

    return 1;
}

static u16 GetDeoxysStat(struct Pokemon *mon, s32 statId)
{
    s32 ivVal, evVal;
    u16 statValue = 0;
    u8 nature;

    if (gBattleTypeFlags & (1 << 5) || GetMonData(mon, 11, ((void *)0)) != 410)
        return 0;

    ivVal = GetMonData(mon, 39 + statId, ((void *)0));
    evVal = GetMonData(mon, 26 + statId, ((void *)0));
    statValue = ((sDeoxysBaseStats[statId] * 2 + ivVal + evVal / 4) * mon->level) / 100 + 5;
    nature = GetNature(mon);
    statValue = ModifyStatByNature(nature, statValue, (u8)statId);
    return statValue;
}

